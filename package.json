{
	"name": "my-pwa-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"packageManager": "bun@1.3.4",
	"dependencies": {
		"estree-walker": "^3.0.3"
	},
	"scripts": {
		"preinstall": "node -e \"const ua = process.env.npm_config_user_agent || ''; if (!ua.includes('bun')) { console.error('This project is locked to bun. Please install dependencies with bun.'); process.exit(1); }\"",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@vite-pwa/sveltekit": "^1.1.0",
		"estree-walker": "^3.0.3",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	}
}
